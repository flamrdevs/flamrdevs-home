---
import clsx from "@flamrdevs/x/modules/clsx";

import type { SVGProps } from "./SVG";

type Props = SVGProps;

const { class: _class, "class:list": classList, ...attrs } = Astro.props;

const className = clsx("fluent-icon", _class, classList);
---

<svg {...attrs} class={className} width="20" height="20" fill="none" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
  <slot />
</svg>
