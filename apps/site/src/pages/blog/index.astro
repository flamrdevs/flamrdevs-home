---
import { getCollection } from "astro:content";

import { HTMLX, Container, Text } from "~/components/core";

const collection = await getCollection("blog");
---

<HTMLX title="blog" description="blog">
  <Container>
    {
      collection.map((item) => (
        <div>
          <Text>{item.slug}</Text>
          <Text>{item.data.title}</Text>
        </div>
      ))
    }
  </Container>
</HTMLX>
